# NetworkManager configuration file for a wifi AP with fixed ipv4 address
[connection]
id=Hotspot
uuid=26a620ec-9cd0-4067-9c92-f9d2b7164c40
type=wifi
interface-name=wlo2
timestamp=1726206429

[wifi]
mode=ap
ssid=raptor-001

[wifi-security]
group=ccmp;
key-mgmt=wpa-psk
pairwise=ccmp;
proto=rsn;
psk=raptor-001

[ipv4]
address1=192.168.0.1/24
method=shared

[ipv6]
addr-gen-mode=stable-privacy
method=ignore

[proxy]
